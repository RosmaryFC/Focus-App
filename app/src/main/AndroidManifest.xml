<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nyc.c4q.rosmaryfc.focus_app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AppMonitor"
            android:label="@string/title_activity_app_monitor" >
        </activity>

        <service android:name=".AppTracker" />
        <service android:name=".BlockSessionReminder" />

        <receiver android:name=".BootReceiver" />

        <activity
            android:name=".AppUsage"
            android:label="@string/title_activity_app_usage" >
        </activity>
    </application>

</manifest>
